{
  "name": "muazzam/slick-scrollbar",
  "description": "Slick Scrollbar plugin for Filament panels (Filament v4).",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "filament/filament": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Muazzam\\SlickScrollbar\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Muazzam\\SlickScrollbar\\SlickScrollbarServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
